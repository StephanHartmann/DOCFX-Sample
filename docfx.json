{
  "metadata": [   
	{
	"src": [
        {
          "files": ["docsample.dll"],
          "src": "dllsource/"
        }
      ],
      "dest": "doc/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "template": ["statictoc"],
    "content": [
      {
        "files": [
		"doc/index.md",
		"doc/toc.yml",
		"doc/api/**.yml",
		"doc/api/index.md",
		"doc/samples/**"
		]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ], 
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  } 
}